{
  "manifest_version": 3,
  "name": "Bilibili Live Clipqueue Extension",
  "version": "1.0.7",
  "description": "An extension to enhance the Bilibili Live Clipqueue experience.",
  "permissions": ["tabs", "activeTab", "storage"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "200": "./assets/favicon.png"
    }
  },
  "icons": {
    "200": "./assets/favicon.png"
  }
}
